{
  "env": {
    "jest": true
  },
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": "off",
    "valid-jsdoc": [
      "error",
      {
        "prefer": {
          "arg": "param",
          "argument": "param",
          "return": "returns"
        }
      }
    ]
  }
}
